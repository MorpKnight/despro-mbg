{
  "expo": {
    "name": "despro-fe",
    "slug": "despro-fe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "desprofe",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.NFC"
      ],
      "package": "com.anonymous.desprofe"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/logo.png",
      "bundler": "metro",
      "name": "MBG-lance",
      "shortName": "MBG",
      "description": "Sistem Monitoring Makanan Bergizi Gratis untuk Sekolah",
      "lang": "id",
      "themeColor": "#2563EB",
      "backgroundColor": "#f5f7fb",
      "display": "standalone",
      "orientation": "portrait",
      "startUrl": "/",
      "scope": "/"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser",
      "react-native-nfc-manager",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": false
    },
    "extra": {
      "apiUrl": "https://mbg-be.mrt.qzz.io/api/v1",
      "MODE": "pusat",
      "auth": {
        "users": [
          {
            "username": "superadmin",
            "password": "password",
            "role": "super_admin"
          },
          {
            "username": "admin_sekolah_test",
            "password": "password",
            "role": "admin_sekolah"
          },
          {
            "username": "admin_catering_test",
            "password": "password",
            "role": "admin_catering"
          },
          {
            "username": "admin_dinkes_test",
            "password": "password",
            "role": "admin_dinkes"
          },
          {
            "username": "siswa_test",
            "password": "password",
            "role": "siswa"
          }
        ],
        "sessionKey": "mbg_auth_session"
      },
      "router": {},
      "eas": {
        "projectId": "c2b333a8-e8ef-4a32-9f3a-b5dbee1b9326"
      }
    },
    "owner": "aliyah.rizky"
  }
}